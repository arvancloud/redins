{
    "handler": {
        "max_ttl": 300,
        "cache_timeout": 60,
        "zone_reload": 600,
        "redis": {
            "ip": "127.0.0.1",
            "port": 6379,
            "password": "",
            "prefix": "test_",
            "suffix": "_test",
            "connect_timeout": 0,
            "read_timeout": 0
        },
        "log": {
            "enable": false
        }
    },
    "upstream": [{
        "ip": "1.1.1.1",
        "port": 53,
        "protocol": "udp",
        "timeout": 1000
    }],
    "geoip": {
        "enable": true,
        "db": "../geoCity.mmdb"
    },
    "error_log": {
        "enable": true,
        "target": "stdout",
        "level": "error",
        "format": "text"
    }
}